            android:text="назад"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button2" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="6dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="2dp" app:layout_constraintTop_toBottomOf="@+id/textView9"
            app:layout_constraintHorizontal_bias="0.085" app:layout_constraintVertical_bias="1.0"
    />
